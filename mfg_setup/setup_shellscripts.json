{
    "pro_2WD_ros1": 
        {
            "mfg_mode": [
                "sudo systemctl stop roverrobotics",
                "sudo systemctl stop roscore",
                "killall roslaunch",
                "killall roscore",
                "source ~/catkin_ws/devel/setup.bash",
                "screen roslaunch roverrobotics_driver mfg_pro_2WD.launch"
            ],
            "normal_mode": [
                "killall roslaunch",
                "killall roscore",
                "killall screen",
                "sudo systemctl start roscore",
                "sudo systemctl start roverrobotics"
            ]
        },
